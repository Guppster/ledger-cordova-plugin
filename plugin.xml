<?xml version="1.0" encoding="UTF-8"?>
<plugin xmlns="http://apache.org/cordova/ns/plugins/1.0" xmlns:android="http://schemas.android.com/apk/res/android" id="me.gurpreetsingh.ledger" version="0.0.1">
    <name>ledger</name>
    <AUTHOR>Gurpreet</AUTHOR>
    <js-module name="ledger" src="www/ledger.js">
        <clobbers target="cordova.plugins.ledger" />
    </js-module>
    <platform name="android">
        <config-file parent="/*" target="res/xml/config.xml">
            <feature name="ledger">
                <param name="android-package" value="me.gurpreetsingh.ledger.ledger" />
            </feature>
        </config-file>
        <config-file parent="/*" target="AndroidManifest.xml" />
        <source-file src="src/android/ledger.java" target-dir="src/me/gurpreetsingh/ledger/ledger" />
    </platform>
</plugin>
